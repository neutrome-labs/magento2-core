<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="NeutromeLabs_Core::config" title="NeutromeLabs Core Configuration"
                                      sortOrder="100"/>
                        </resource>
                    </resource>
                </resource>
                <resource id="NeutromeLabs_Core::system" title="NeutromeLabs" sortOrder="90">
                    <resource id="NeutromeLabs_Core::settings" title="NeutromeLabs Settings" sortOrder="10">
                        <resource id="NeutromeLabs_Core::config_cloud" title="Cloud Account Configuration"
                                  sortOrder="10"/>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
